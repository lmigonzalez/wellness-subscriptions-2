{
  "crons": [
    {
      "path": "/api/cron/daily-plan",
      "schedule": "0 9 * * *"
    }
  ],
  "functions": {
    "app/api/daily-plan/route.ts": {
      "maxDuration": 30
    }
  },
  "headers": [
    {
      "source": "/api/daily-plan",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache, no-store, must-revalidate"
        },
        {
          "key": "Pragma",
          "value": "no-cache"
        },
        {
          "key": "Expires",
          "value": "0"
        }
      ]
    }
  ]
}
